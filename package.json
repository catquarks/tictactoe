{
  "name": "tictactoe",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.2",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "immutability-helper": "^2.1.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "npm-sass ./src/_index.scss > ./src/index.css",
    "watch-css": "nodemon -e scss -x  \"npm run build-css\"",
    "deploy": "npm run build && deploy-site tictactoe/build tictactoe"
  },
  "homepage": "http://comfortcat.xyz/tictactoe"
}
